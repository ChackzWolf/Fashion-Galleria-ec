    // if user press any key and release
    input.onkeyup = (e) => {
        let userData = e.target.value; //user enetered data
        let emptyArray = [{}];
        if (userData) {
            $.ajax({
                url: '/search',
                type: 'GET',
                data: { search: userData },
                success: function (data) {
                    if (data.length > 0) {

                        emptyArray = data.map(function (val) {
                            return (val);
                        });

                        emptyArray = emptyArray.map((data) => {


                            // passing return data inside li tag
                            return data = '<li> <a href="/product-details?id=' + data._id + '">' + data.name + '</a></li>';
                        });

                        SearchResult.style.display = "block"; //show autocomplete box
                        showSuggestions(emptyArray);
                        let allList = resultBox.querySelectorAll("li");
                        for (let i = 0; i < allList.length; i++) {
                            //adding onclick attribute in all li tag
                            allList[i].setAttribute("onclick", "select(this)");
                        }
                    } else {

                        data = '<ul> </ul>';
                        resultBox.innerHTML = data;
                    }
                }
            })
        }
        else {
            SearchResult.style.display = "none"; //hide autocomplete box
        }
    }

    const elem = document.getElementById('inputBar')
    elem.onmouseover = function () {
        SearchResult.style.display = "block";
    }


    elem.onmouseout = function () {
        SearchResult.style.display = "none";
    }

